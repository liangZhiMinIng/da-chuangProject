
.container{
  width: 100%;
  top: 0rpx;
  /* height: 1000rpx; */
  /* background-color: yellowgreen; */
  padding: 20rpx;
}
.scan{
  width: 100%;
  height:500rpx;
  background-color:#F0F8FF;
  margin-top: 100rpx;
  border-radius: 26rpx;
  box-shadow: rgb(0, 0, 0 /0.7);
  display: flex;
  justify-content: center;
  align-items: center;
}
.map_module{
  display: flex;
  justify-content: center;
  align-items: center;
}
.map{
  width: 100%;
  height: 500rpx;
  margin-top: 100rpx;
  background-color: #F0F8FF;
  border-radius: 26rpx;
}
.sound{
  margin-top: 20rpx;
  box-sizing: border-box;
  padding-top: 100rpx;
  text-align: center;
  width: 100%;
  height: 350rpx;
  border-radius: 10rpx;
  background-color: #F0F8FF;
}
.context{
  width: 100%;
  height: 80rpx;
  margin-top: 50rpx;
  background-color: #F0F8FF;
  border: 0.2rpx solid black;
}